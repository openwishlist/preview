{
  "name": "@openwishlist/preview",
  "version": "0.105.0",
  "description": "Serverless link preview api",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "deploy": "serverless deploy",
    "develop": "serverless offline start",
    "db:migrate": "serverless dynamodb migrate",
    "db:explorer": "dynamodb-admin",
    "doc:generate": "serverless openapi generate -o spec/openapi.json -f json -a 3.0.3",
    "doc:convert": "typeconv -f ts -t oapi -o spec/models --oapi-format yml 'src/models/*.ts'",
    "watch": "tsc -w",
    "lint": "eslint .",
    "format": "prettier --config .prettierrc 'src/**/*.ts' 'test/**/*.ts' --write"
  },
  "keywords": [
    "serverless",
    "html",
    "scraping"
  ],
  "author": "Milind Singh",
  "license": "MIT",
  "devDependencies": {
    "@shelf/jest-dynamodb": "3.3.1",
    "@types/jest": "29.2.3",
    "@types/node": "16.18.3",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "aws-sdk": "2.1261.0",
    "dynamodb-admin": "4.5.0",
    "eslint": "8.28.0",
    "jest": "29.3.1",
    "prettier": "2.8.0",
    "puppeteer": "13.7.0",
    "serverless-dynamodb-local": "0.2.40",
    "serverless-offline": "11.6.0",
    "serverless-openapi-documenter": "0.0.27",
    "serverless-plugin-typescript": "2.1.4",
    "serverless-tscpaths": "0.0.7",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typeconv": "1.8.0",
    "typescript": "4.9.3"
  },
  "dependencies": {
    "@types/lodash": "^4.14.185",
    "@types/user-agents": "^1.0.2",
    "aws-lambda": "^1.0.7",
    "axios": "^1.0.0",
    "base64url": "^3.0.1",
    "cheerio": "^1.0.0-rc.12",
    "chrome-aws-lambda": "^10.1.0",
    "config": "^3.3.7",
    "lodash": "^4.17.21",
    "puppeteer-core": "13.7.0",
    "schema-dts": "^1.1.0",
    "serverless-api-gateway-throttling": "^2.0.2",
    "user-agents": "^1.0.1102",
    "uuid": "^9.0.0"
  },
  "engines": {
    "node": "16.x",
    "npm": "6.x"
  }
}
